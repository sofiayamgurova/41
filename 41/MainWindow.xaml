<Window x:Class="_41.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_41"
        mc:Ignorable="d"
        Title="Ямгурова Софа" Height="450" Width="800" FontFamily="Comic Sans MS" Icon="res/icon.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="res/logo.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="41 размер" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered"
               NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="ButtonBack" HorizontalAlignment="Right" Click="ButtonBack_Click"></Button>
    </Grid>
</Window>
